{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "RabbitMQHost": "localhost",
    "RabbitMQUser": "user",
    "RabbitMQPassword": "pass",
    "RabbitMQVhost": "vhost",

    "MqttHost": "localhost",
    "MqttUser": "user",
    "MqttPass": "pass",
    "MqttPort": 1883 
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "mqtt-service",
    "address": "docker.for.win.localhost",
    "port": "8066",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 15,
    "removeAfterInterval": 240,
    "requestRetries": 3
  },
  "restEase": { "loadBalancer": "fabio" },
  "fabio": { "url": "http://localhost:9999" },
  "Seq": {
    "ServerUrl": "http://localhost:5341",
    "ApiKey": null,
    "MinimumLevel": "Information",
    "LevelOverride": {
      "Microsoft": "Warning"
    }
  }
}
