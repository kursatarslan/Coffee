{
	"Logging": {
		"LogLevel": {
			"Default": "Warning"


		}
	},
	"Kestrel": {
		"EndPoints": {
			"Http": {
				"Url": "http://localhost:80"
			}
		}
	},
	"AllowedHosts": "*",
	"ConnectionStrings": {
		"RabbitMQHost": "rabbit",
		"RabbitMQUser": "user",
		"RabbitMQPassword": "pass",
		"RabbitMQVhost": "vhost",

		"MqttHost": "host",
		"MqttUser": "user",
		"MqttPass": "pass",
		"MqttPort": 1883 
	},
	"consul": {
		"enabled": true,
		"url": "http://consul:8500",
		"service": "mqtt-service",
		"address": "mqtt-service",
		"port": "80",
		"pingEnabled": true,
		"pingEndpoint": "ping",
		"pingInterval": 30,
		"removeAfterInterval": 240,
		"requestRetries": 10
	},
	"restEase": { "loadBalancer": "fabio" },
	"fabio": { "url": "http://fabio:9999" },
	"Seq": {
		"ServerUrl": "http://seq:5341",
		"ApiKey": null,
		"MinimumLevel": "Information",
		"LevelOverride": {
			"Microsoft": "Warning"
		}
	}
}
